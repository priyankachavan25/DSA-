#include <stdio.h>

int Sum(int arr[], int size) 
{
    int sum = 0;
    for (int i = 0; i < n; i++) 
   {
        sum += arr[i];
    }
    return sum;
}

int main()
 {
    int array[] = {1, 2, 3, 4, 5};
    int n = sizeof(array) / sizeof(array[0]);
    int result = Sum(array, n);
    printf("Sum: %d\n", result);  
    return 0;
}
